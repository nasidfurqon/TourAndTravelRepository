@model IEnumerable<Tempat>
@{
    ViewData["Title"] = "Tempat Wisata";
}
        <table class="table ">
            <thead>
            <tr>
                <th scope="col">Tempat Wisata</th>
                <th scope="col">Kota </th>
                <th scope="col">Keterangan </th>
            </tr>
            </thead>
    @foreach (var item in Model)
    {
            <tbody>
            <tr>
                <td>@item.NamaTempat</td>    
                <td>@item.Kota</td>
                <td><a asp-controller="TourAndTravel" asp-action="Show" asp-route-id="@item.Id">Details</a> </td>
            </tr>
            </tbody>
    }
        </table>